{
  "name": "Cache Wipe",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Clear your cache, fast and easy. Upon clicking the browser icon, your cache will clear then the tab will reload.",
  "icons": {
    "16": "media/icon(beta).png",
    "48": "media/icon(beta).png",
    "128": "media/icon(beta).png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_page": "options/options.html",
  "browser_action": {
    "default_icon": {
      "19": "media/icon(beta).png",
      "38": "media/icon(beta).png"
    },

    "default_title": "Cache Clear"
  },

  "permissions": ["browsingData"]
}
